
#include <stdio.h>

int main()
{
  puts("hello");
  fputs("type something bro... idgaf whatever is cool",stdout);
  int input;
  if ( input == EOF){
    goto done;
  }
  switch(input){
  case 'p':
    puts("you are pushing p!....");
    puts("you a playa");
    break;
  default:
    puts("you NOT P");
}
